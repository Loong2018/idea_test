<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="site.laoda.mapper.UsersMapper">

    <insert id="insertUser" parameterType="site.laoda.pojo.Users">
        insert into users(username,userage) values(#{username}, #{userage})
    </insert>

    <select id="selectUserAll" resultType="site.laoda.pojo.Users">
        select * from users
    </select>

</mapper>
